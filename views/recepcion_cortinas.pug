if arrAsnRecCor != undefined
  each item in arrAsnRecCor
    .col-xl-3.col-md-6.mb-4
      .card.border-left-primary.shadow.h-100.py-2
        .card-body
          .row.no-gutters.align-items-center
            .col.mr-2.card-warehouse(id='div_almacen_' + item["id_almacen"])
              .text-xs.font-weight-bold.text-uppercase.mb-1 #{item["almacen"]}
              .h6.mb-0.text-monospace.font-weight-bold.text-gray-800 Cortinas Disp 
                .span.d-inline-block #{item["cortinas"] - item["operacion"]}
              .h6.mb-0.text-monospace.font-weight-bold.text-gray-800 Cortinas Ocup 
                .span.d-inline-block #{item["operacion"]}