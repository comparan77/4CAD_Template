.card.shadow.mb-4
  .card-header.py-3
    h6.m-0.font-weight-bold.text-primary Recibidos por ubicar
  .card-body
    .table-responsive
      table.table.table-bordered(id="dataTable" width="100%" cellspacing="0")
        thead
          tr
            th Folio
            th Cliente
            th Producto
            th Cantidad
            th Ubicado(s)
            th Pendiente(s)
            th % Ubicados
        tfoot
          tr
            th Folio
            th Cliente
            th Producto
            th Cantidad
            th Ubicado(s)
            th Pendiente(s)
            th % Ubicados
        tbody
          if lstRec != undefined
            each item in lstRec
              tr
                td
                  a.nav-lingk.folio-ent(href="#") 
                    span #{item.Folio}
                  input(type="hidden" value='key_' + item.Id_entrada)
                td #{item.Cliente}
                td #{item.Producto}
                td.text-center #{item.Cantidad + ' ' + item.Formato + (item.Cantidad > 1 ? 's' : '')} 
                td.text-center #{item.Ubicados}
                td.text-center #{item.Pendientes}
                td.text-center #{Math.round(item.Ubicados / item.Cantidad * 100,0)}